<tr>
  <td><%= project.name %></td>
  <td><%= project.description %></td>
  <td><%= project.start_date.strftime("%Y-%m-%d") if project.start_date %></td>
  <td><%= project.end_date.strftime("%Y-%m-%d") if project.end_date %></td>
  <td><%= project.status %></td>
  <td>
    <%= link_to 'View', project_path(project), class: 'btn btn-info btn-sm' %>
    <%= link_to 'Edit', edit_project_path(project), class: 'btn btn-warning btn-sm' %>
    <%= link_to 'Delete', project_path(project), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger btn-sm' %>
  </td>
</tr>
