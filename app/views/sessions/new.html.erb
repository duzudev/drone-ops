<h3>Sign In</h3>

<%= form_with url: sign_in_path do |form| %>

    <div class="mb-3">
        <%= form.label :email%>
        <%= form.text_field :email, class: "form-control", placeholder: "duzu@arcoworldwide.ng"%>
    </div>
    <div class="mb-3">
        <%= form.label :password%>
        <%= form.password_field :password, class: "form-control", placeholder: "password"%>
    </div>

    <div class="mb-3">
        <%= form.submit "Log In", class: "btn btn-primary"%>
    </div>

<% end %> 